<body>

    <h1>C치lculo do Custo do Carro Novo</h1>

    <script>
        function calcularCustoCarro() {
            
            const percentualRevendedor = 0.25; // 25%
            const percentualImpostos = 0.45; // 45%

           
            let custoFabrica = parseFloat(prompt("Digite o custo de f치brica do carro: R$"));

        
            let custoFinal = custoFabrica + (custoFabrica * percentualRevendedor) + (custoFabrica * percentualImpostos);

            
            alert("Custo de F치brica: R$ " + custoFabrica.toFixed(2));
            alert("Percentual do Revendedor (25%): R$ " + (custoFabrica * percentualRevendedor).toFixed(2));
            alert("Impostos (45%): R$ " + (custoFabrica * percentualImpostos).toFixed(2));
            alert("Custo Final ao Consumidor: R$ " + custoFinal.toFixed(2));
        }

        
        calcularCustoCarro();
    </script>

</body>
